    "@babel/plugin-transform-classes": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.8.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.8.0",
    "@babel/runtime-corejs2": "^7.8.0",
    "@babel/runtime-corejs3": "^7.8.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
